	Keywords = {census; lynx; monitoring; population simulations; reproduction SPATIAL-ORGANIZATION; EURASIAN LYNX; BEHAVIOR},
	Number = {4},
	Pages = {299-306},
	Title = {Estimating total lynx \emph{Lynx lynx} population size from censuses of family groups},
	Volume = {8},
